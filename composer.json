{
  "name": "traw/vcfqr",
  "description": "Create vcf card with QR Code",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3 CMS",
    "VCF",
    "QR-Code"
  ],
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "TRAW\\Vcfqr\\": "Classes/"
    }
  },
  "require": {
    "typo3/cms-fluid": "^12 || ^13",
    "friendsoftypo3/tt-address": "^8.0",
    "giggsey/libphonenumber-for-php": "^8.12",
    "chillerlan/php-qrcode": "^5.0",
    "sabre/vobject": "^4.0"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "vcfqr"
    }
  }
}
